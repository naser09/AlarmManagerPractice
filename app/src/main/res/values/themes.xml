<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="Theme.FiverrMorbedPractice" parent="android:Theme.Material.Light.NoActionBar">
        <item name="android:statusBarColor">@color/purple_700</item>
    </style>

    <style name="Theme.FiverrMorbedPractice.Fullscreen" parent="Theme.FiverrMorbedPractice">
        <item name="android:actionBarStyle">
            @style/Widget.Theme.FiverrMorbedPractice.ActionBar.Fullscreen
        </item>
        <item name="android:windowActionBarOverlay">true</item>
        <item name="android:windowBackground">@null</item>
    </style>

    <style name="ThemeOverlay.FiverrMorbedPractice.FullscreenContainer" parent="">
        <item name="fullscreenBackgroundColor">@color/light_blue_600</item>
        <item name="fullscreenTextColor">@color/light_blue_A200</item>
    </style>
</resources>